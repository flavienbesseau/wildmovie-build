() {\n    const { infoFavorites, infoAlreadySeen } = this.state;\n    return (\n      <>\n        <Container>\n          <H1>Favorite List</H1>\n          <Div>\n            {infoFavorites.map((essai) => (\n              <Movie\n                posterPath={essai.poster_path}\n                title={essai.original_title}\n                id={essai.id}\n              />\n            ))}\n          </Div>\n          <H1>Already seen</H1>\n          <Div>\n            {infoAlreadySeen.map((essai2) => (\n              <Movie\n                posterPath={essai2.poster_path}\n                title={essai2.original_title}\n                id={essai2.id}\n              />\n            ))}\n          </Div>\n        </Container>\n        <FixFooter>\n          <Footer />\n        </FixFooter>\n      </>\n    );\n  }\n}\n\nMyList.propTypes = {\n  alreadySeen: PropTypes.arrayOf(PropTypes.object).isRequired,\n  favorites: PropTypes.arrayOf(PropTypes.object).isRequired,\n};\n\nexport default MyList;\n","import React, { Component } from \"react\";\nimport {\n  Collapse,\n  Navbar,\n  NavbarToggler,\n  NavbarBrand,\n  Nav,\n  NavItem,\n  NavLink,\n  UncontrolledDropdown,\n  DropdownMenu,\n  DropdownItem,\n  DropdownToggle,\n} from \"reactstrap\";\n\nclass NavbarComponent extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      isOpen: false,\n    };\n  }\n\n  toggleNavBar = () => {\n    const { isOpen } = this.state;\n    this.setState({ isOpen: !isOpen });\n  };\n\n  render() {\n    const { isOpen } = this.state;\n    return (\n      <Navbar color=\"dark\" light expand=\"md\" className=\"nav\">\n        <NavbarBrand href=\"/\" style={{ width: \"40%\" }}>\n          <img\n            src=\"https://i.ibb.co/PFVmqVP/Wild-Movie-logo.png\"\n            alt=\"Wild Movie\"\n            className=\"logo\"\n          />\n        </NavbarBrand>\n        <NavbarToggler\n          onClick={this.toggleNavBar}\n          style={{ background: \"#b65ce7\", opacity